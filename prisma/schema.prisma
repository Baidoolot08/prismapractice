
generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id        Int      @id @default(autoincrement())
  name      String
  age       Int
  personUrl String   @default("https://example.com/default.jpg")
  hobby     String   @default("Unknown")
  createAt  DateTime @default(now())
  updatedAt DateTime @updatedAt
}
